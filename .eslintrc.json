{
    "globals": {
        "document": true,
        "window": true
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "prefer-destructuring": ["error", {
            "array": false,
            "object": true
        }, {
            "enforceForRenamedProperties": false
        }]
    },
    "extends": "airbnb-base"
}